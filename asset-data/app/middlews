import { NextResponse } from "next/server";

import { isAuth } from './utils/auth'

export function middleware(req) {
  const user = isAuth()
  const role = user?.role


  if (role !== 'user') {
    return NextResponse.redirect(new URL(req.url))
  }


  return NextResponse.redirect(new URL(req.url))
}


export const config = {
  matcher: ['/asset/:path*', '/myprivate/:path*'],
}